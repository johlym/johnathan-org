<header class="container">
  <nav class="navbar navbar-expand-lg navbar-light bg-white">
    <div class="container-fluid">
      <a class="navbar-brand me-3" href="/"> <svg width="120px" height="60px"
          viewBox="0 0 272 54" version="1.1" xmlns="http://www.w3.org/2000/svg"
          xmlns:xlink="http://www.w3.org/1999/xlink">
          <g id="Page-1" stroke="none" stroke-width="1" fill="none"
            fill-rule="evenodd">
            <g id="rubyshape" transform="translate(1.000000, -7.000000)"
              fill="#000000"> <text id="Bootiful" font-size="48"
                font-weight="700" line-spacing="69" letter-spacing="1.80000007">
                <tspan x="94" y="51">Logo</tspan>
              </text>
              <polygon id="railslike" stroke="#000000" fill-rule="nonzero"
                transform="translate(29.000000, 34.500000) rotate(-180.000000) translate(-29.000000, -34.500000) "
                points="29 8 58 44.9857295 46.9229857 61 11.0770143 61 0 44.9857295">
              </polygon>
            </g>
          </g>
        </svg> </a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
        data-bs-target="#actualNavbar" aria-controls="actualNavbar"
        aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="actualNavbar">
        <ul class="navbar-nav me-auto mb-2 mb-md-0">
          {% for item in nav %}
          <li class="nav-item">
            <a class="nav-link fw-500" aria-current="page"
              href="/{{ item.path }}">{{ item.name }}</a>
          </li>
          {% endfor %}
        </ul>
        <div class="ms-auto navbar-nav">
          <a href="/login" class="nav-link fw-500 me-3">Sign in</a>
          <a class="btn btn-dark text-white fw-500"
            href="https://github.com/bdavidxyz/bootiful">
            Start on Github
          </a>
        </div>
      </div>
  </nav>
</header>