<!doctype html>
<html lang="{{ site.locale }}">

<head>
  {% render "head", metadata: site.metadata, title: page.title %}
</head>

<body class="site {{ resource.data.layout }} {{ resource.data.page_class }}">
  {% render "navbar", metadata: site.metadata, 
                        resource: resource, 
                        nav: site.data.nav %}

  <div class="main">
    <div class="container px-0">
      <div class="row">
        <div class="col col-lg mx-auto">
          {{ content }}
        </div>
      </div>

    </div>
  </div>

  {% render "footer", metadata: site.metadata, nav: site.data.nav, nav2: site.data.nav_second %}
</body>

</html>